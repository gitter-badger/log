{
  "name": "todo",
  "version": "0.0.1",
  "scripts": {
    "test": "lab -cv -t 100 -T test/transform.js",
    "start": "make server",
    "stop": "sudo pkill python",
    "install": "make install",
    "prefixer": "postcss --use autoprefixer browser/public/main.css -d browser/public",
    "build-js": "browserify -t babelify browser/components/app.jsx > browser/public/app.js",
    "build-css": "node-sass browser/stylesheets/main.sass > browser/public/main.css && npm run prefixer"
  },
  "devDependencies": {
    "autoprefixer": "^5.2.0",
    "babel-core": "^5.7.4",
    "babelify": "^6.1.3",
    "browserify": "^10.2.6",
    "code": "^1.4.1",
    "jsdom": "^5.6.1",
    "knwl.js": "^1.0.2",
    "lab": "^5.13.0",
    "localforage": "^1.2.4",
    "moment": "^2.10.3",
    "node-sass": "^3.2.0",
    "postcss-cli": "^1.4.0",
    "react": "^0.13.3"
  }
}
