{
  "name": "todo",
  "version": "0.0.1",
  "scripts": {
    "test": "lab -cv -t 100 -T test/transform.js",
    "start": "make server",
    "install": "make install",
    "prefixer": "postcss --use autoprefixer browser/bundle/main.css -d browser/bundle",
    "bundle-js": "browserify -t babelify browser/src/components/app.jsx > browser/bundle/app.js",
    "bundle-css": "node-sass browser/src/stylesheets/main.sass > browser/bundle/main.css && npm run prefixer"
  },
  "devDependencies": {
    "autoprefixer": "^5.2.0",
    "babel-core": "^5.7.4",
    "babelify": "^6.1.3",
    "browserify": "^10.2.6",
    "code": "^1.4.1",
    "jsdom": "^5.6.1",
    "lab": "^5.13.0",
    "node-sass": "^3.2.0",
    "postcss-cli": "^1.4.0",
    "react": "^0.13.3"
  }
}
